    '' ,
    0 ,
    '' ,
    0
FROM
    dmi_queue_item_s dmi_queue_item
WHERE
    (
        dmi_queue_item.name IN
        (
            SELECT DISTINCT gr2.i_supergroups_names
            FROM dm_group_r gr1,
            dm_group_r gr2
            WHERE gr1.r_object_id = gr2.r_object_id
            AND gr1.users_names = :P19
        )
    )
    AND dmi_queue_item.delete_flag = 0
    AND dmi_queue_item.dependency_type IN ('0', '1', '2', '3', '4', '5', '6')
    AND NOT EXISTS
    (
        SELECT 1
        FROM dmi_package_s ps
        WHERE dmi_queue_item.router_id = ps.r_workflow_id
        AND dmi_queue_item.task_number = ps.r_act_seqno
    )
ORDER BY 5 ASC,
